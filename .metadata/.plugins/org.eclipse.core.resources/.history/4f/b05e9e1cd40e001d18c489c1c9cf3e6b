package functionPackage;

public class AdditionDoubleNumber {
	
	static int a = 10;
	static int b = 20;
	
	public static void addTwoDoubleNumbers(double numOne, double numTwo) {
		double result  = numOne + numTwo;
		System.out.println(result);
	}
	
	public void addThreeDoubleNumbers(double numOne, double numTwo, double numThree) {
		double result  = numOne + numTwo + numThree;
		System.out.println(result);
	}

}
